{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "index.js"
    ],
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "node_modules/minami",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": true,
    "sort": true
  }
}